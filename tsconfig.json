{
  "compilerOptions": {
    "jsx": "react",
    "target": "ES2019",
    "strict": true,
    "strictFunctionTypes": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noErrorTruncation": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "baseUrl": "packages/client/src"
  }
}
